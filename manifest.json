{
  "manifest_version": 2,
  "name": "YTMusic Unity Sender",
  "version": "1.1",
  "description": "Sends YouTube Music playback data to Unity Editor.",
  
  "permissions": [
    "tabs",
    "http://localhost:8080/"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://music.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}